{
  "name": "mod-template-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "build-cli": "node build-cli.js",
    "run": "npm run build-cli && node build/cli.js",
    "test-generate-mdg": "npm run run -- generate --mod-name 'Test Mod' --mod-id testmod --minecraft-version 1.21.4 --package-name net.neoforged.testmod --output-folder out",
    "test-generate-ng": "npm run run -- generate --mod-name 'Test Mod' --mod-id testmod --minecraft-version 1.21.4 --package-name net.neoforged.testmod --gradle-plugin ng --output-folder out"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@xmldom/xmldom": "^0.9.6",
    "commander": "^12.1.0",
    "file-saver": "^2.0.5",
    "glob": "^11.0.0",
    "jszip": "^3.10.1",
    "mustache": "^4.2.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "esbuild": "^0.24.2",
    "prettier": "3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vue-tsc": "^2.2.0"
  }
}
